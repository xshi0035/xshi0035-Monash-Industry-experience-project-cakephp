{
    "name": "ugie-cake/cakephp-content-blocks",
    "description": "ContentBlocks plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "cakephp/cakephp": "5.0.*",
        "ext-json": "*",
        "ezyang/htmlpurifier": "^4.16"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.3"
    },
    "autoload": {
        "psr-4": {
            "ContentBlocks\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ContentBlocks\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
